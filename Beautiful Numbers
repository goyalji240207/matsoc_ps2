#include <iostream>
#include <vector>
using namespace std;

long long MOD = 1000000007;

bool is_good(int sum, int a, int b) {
    while (sum > 0) {
        int digit = sum % 10;
        if (digit != a && digit != b) return false;
        sum /= 10;
    }
    return true;
}

long long power(long long base, long long exp) {
    long long res = 1;
    base %= MOD;
    while (exp > 0) {
        if (exp % 2 == 1) res = (res * base) % MOD;
        base = (base * base) % MOD;
        exp /= 2;
    }
    return res;
}

long long modInverse(long long n) {
    return power(n, MOD - 2);
}

int main() {
    int a, b, n;
    if (!(cin >> a >> b >> n)) return 0;

    vector<long long> fact(n + 1);
    fact[0] = 1;
    for (int i = 1; i <= n; i++) {
        fact[i] = (fact[i - 1] * i) % MOD;
    }

    long long total_excellent = 0;

    // Iterate through all possible counts of digit 'a'
    for (int i = 0; i <= n; i++) {
        int sum = i * a + (n - i) * b;
        
        if (is_good(sum, a, b)) {
            // Calculate nCi = n! / (i! * (n-i)!)
            long long num = fact[n];
            long long den = (fact[i] * fact[n - i]) % MOD;
            long long nCr = (num * modInverse(den)) % MOD;
            
            total_excellent = (total_excellent + nCr) % MOD;
        }
    }

    cout << total_excellent << endl;

    return 0;
}
